{
  "author": "riverstart",
  "name": "vite-pug",
  "version": "0.0.0",
  "description": "Starter project template boilerplate based on Vite with SASS/PostCSS, pug and babel ES6/7 support.",
  "browserslist": [
    "last 2 versions and > 1%",
    "not dead",
    "safari >= 15.6",
    "ios_saf >= 15.6",
    "not ie > 0"
  ],
  "devDependencies": {
    "@babel/core": "^7.25.7",
    "@babel/eslint-parser": "^7.24.1",
    "@babel/preset-env": "^7.24.3",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@spiriit/vite-plugin-svg-spritemap": "^2.2.4",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vituum/vite-plugin-pug": "^1.1.0",
    "autoprefixer": "^10.4.19",
    "colorjs.io": "^0.5.0",
    "critical": "^7.2.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "fs-extra": "^11.2.0",
    "husky": "^9.0.11",
    "js-beautify": "^1.15.1",
    "lint-staged": "^15.2.10",
    "postcss-extract-media-query": "^3.0.0",
    "prettier": "^3.2.5",
    "sass": "^1.80.5",
    "style-dictionary": "^3.9.2",
    "tinycolor2": "^1.6.0",
    "token-transformer": "^0.0.33",
    "vite": "^5.4.10",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-clean": "^1.0.0",
    "vite-plugin-clean-css": "^0.0.3",
    "vite-plugin-restart": "^0.4.1",
    "vituum": "^1.1.0",
    "vkbeautify": "^0.99.3"
  },
  "engines": {
    "node": ">=21"
  },
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git@git.rs-app.ru:rs/html/frontend-vite-pug.git"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "vite build && node ./generate-critical-css.js && node ./pretty-html.js",
    "build:tokens": "node ./build-figma-tokens.js",
    "build:critical": "node ./generate-critical-css.js",
    "build:pretty-html": "node ./pretty-html.js",
    "preview": "vite preview",
    "analyzer": "npx vite-bundle-visualizer --output ./analyzer.html",
    "lint": "eslint ./src/js --ext .js,.vue",
    "lint:fix": "eslint ./src/js --fix --ext .js,.vue",
    "prepare": "husky",
    "lint-staged": "lint-staged"
  },
  "target": "web",
  "dependencies": {
    "@sentry/browser": "^7.108.0",
    "@sentry/tracing": "^7.108.0",
    "air-datepicker": "^3.2.12",
    "axios": "^1.7.7",
    "dayjs": "^1.11.13",
    "focus-visible": "^5.2.0",
    "inputmask": "5.0.7",
    "intl-tel-input": "^18.5.3",
    "lodash": "^4.17.21",
    "macy": "^2.5.1",
    "mustache": "^4.2.0",
    "seamless-scroll-polyfill": "^2.3.4",
    "swiper": "^11.1.14",
    "tom-select": "^2.1.2",
    "vue": "^3.5.12",
    "ymaps": "^0.2.1"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,vue}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
